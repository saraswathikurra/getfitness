tton type=\"submit\" class=\"button is-dark is-large is-fullwidth has-text-centered\">Login</button><br>\r\n                    <div class=\"has-text-centered\">\r\n                        <div class=\".has-margin-top-5\"> Don't have an account? </div>\r\n                        <div> Click <router-link to=\"/register\"> here </router-link> to register</div> \r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name: \"Login\",\r\n    props: {\r\n        apiUrl: {\r\n            type: String,\r\n            default: \"http://localhost:3000/\"\r\n        }\r\n    },\r\n\r\n    data(){\r\n        return {\r\n            user:{\r\n                email: \"\",\r\n                password: \"\"\r\n            },\r\n            error: '',\r\n            isLogin: localStorage.getItem(\"isLogin\")\r\n        }\r\n    },\r\n    created() {\r\n        if (this.isLogin === \"true\") {\r\n            this.$router.push(\"/home\");\r\n        }\r\n    },\r\n    methods: {\r\n        login(){\r\n        axios.post(`${this.apiUrl}user/login`, this.user)\r\n            .then(res => {\r\n                if (res.statusText === \"OK\") {\r\n                    localStorage.setItem(\"user\", JSON.stringify(res.data));\r\n                    localStorage.setItem(\"isLogin\", \"true\");\r\n                    this.$router.push(\"/index\");\r\n                    this.$router.go()\r\n                } else {\r\n                }\r\n            })\r\n            .catch(err => console.log(err));\r\n        }\r\n    }\r\n}\r\n</script>"]}]}