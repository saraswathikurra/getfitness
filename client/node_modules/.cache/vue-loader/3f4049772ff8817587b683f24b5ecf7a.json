urn this.userInput = this.orgUserInput\r\n      },\r\n      sumbitPost: function() {\r\n        if (this.userInput.Message == '' || this.userInput.Name == '' || this.userInput.Email == '') {\r\n          return this.formError = true\r\n        } else {\r\n          this.formError = false,\r\n          this.$http.post('', \r\n          {\r\n            body: this.userInput.Message,\r\n            userId: 1\r\n          }).then(function(data){\r\n            this.showForm = false\r\n            this.clearPost()\r\n          })\r\n        }\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}